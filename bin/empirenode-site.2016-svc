#!/usr/bin/env node

'use strict';

const log = require('llog');

log.info('starting empirenode-site.2016-svc');

require('errortrap')();

const api = require('../api');
const service = require('../service');

api.start((err) => {
  if (err) throw err;
  service.start((err) => {
    if (err) throw err;
    log.info('empirenode-site.2016-svc started');
  });
});
